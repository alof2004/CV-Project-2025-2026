[sub_resource type="Shader" id="Shader_dunqq"]
code = "shader_type spatial;

uniform sampler2D texture_albedo : source_color, filter_nearest_mipmap;
uniform float saturation : hint_range(0.0, 3.0) = 0.8; // Adjustable slider

void fragment() {
    vec4 albedo_tex = texture(texture_albedo, UV);
    
    // Convert RGB to Grayscale
    float verify_grayscale = dot(albedo_tex.rgb, vec3(0.299, 0.587, 0.114));
    
    // Mix the grayscale version with the original color based on saturation amount
    ALBEDO = mix(vec3(verify_grayscale), albedo_tex.rgb, saturation);
}"

